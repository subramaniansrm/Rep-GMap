    -o-transition: color .2s ease-out;
    transition: color 1s; /* For modern browsers */
    border-bottom: 1px solid transparent;
}
.readMore:hover {color:#ed7d31;}
.readMore::after {
    content: '';
    width: 0px;
    height: 2px;
    display: block;
    background: #ed7d31;
    transition: 300ms;
}
.readMore:hover::after {width: 100%;}


